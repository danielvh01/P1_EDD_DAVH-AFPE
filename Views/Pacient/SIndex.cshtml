@model IEnumerable<DataStructures.HeapNode<PacientModel>>
@using P1_EDD_DAVH_AFPE.Models.Data 


<h1>Lista de espera</h1>

<a asp-action="Index">Regresar a menu principal</a>

<table class="table">
    <thead>
        <tr>
            <th>
                Nombre
            </th>
            <th>
                Apellido
            </th>
            <th>
                DPI / Partida de nacimiento
            </th>
            <th>
                Departamento
            </th>
            <th>
                Municipio
            </th>
            <th>
                Prioridad
            </th>
            <th>
                Horario
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.value.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.value.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.value.DPI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.value.Department)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.value.municipality)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.value.priority)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.value.schedule)
            </td>
            <td>
            <td>
                @Html.ActionLink("Vaccinated", "Delete", new { item.value.DPI })
                @Html.ActionLink("Did not assist", "Reschedule", new { item.value.DPI }) |
            </td>
            </td>
        </tr>
        }
    </tbody>
</table>
