
@{
    ViewData["Title"] = "Login";
}

<h1>Login</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <select class="form-select" asp-for="department">
                    @foreach (var item in P1_EDD_DAVH_AFPE.Models.Data.Singleton.Instance.department)
                    {

                        <option value="@item">@Html.DisplayFor(modelItem => item)</option>

                    }
                </select>
            </div>
            <div class="form-group">
                <select class="form-select" asp-for="municipalities">
                    @foreach (var item in P1_EDD_DAVH_AFPE.Models.Data.Singleton.Instance.municipalities)
                    {

                        <option value="@item">@Html.DisplayFor(modelItem => item)</option>

                    }
                </select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>